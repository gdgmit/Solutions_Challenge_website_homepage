	:root {
		--hover-top: 10px;
		--hover-bottom: -10px;
	}

	#robot {
		top: 50%; /* Adjust position for better visibility */
		left: 75%; /* Shift slightly to the right */
		max-width: 60vw;
		max-height: 70vh;
	}


/* Default animations for all screen sizes */
@keyframes hover {
  0% {
    transform: translate3d(0, var(--hover-top, 6px), 0); /* Reduced translation for smaller screens */
  }
  100% {
    transform: translate3d(0, var(--hover-bottom, -6px), 0); /* Reduced translation for smaller screens */
  }
}

@keyframes shadow {
  0% {
    transform: translate3d(0, 0, 0) scale(1.3, 1.1); /* Slightly smaller scaling for mobile */
    opacity: 0.35; /* Reduced opacity for subtle effect */
  }
  100% {
    transform: translate3d(0, 0, 0) scale(1, 1);
    opacity: 0.2;
  }
}

@keyframes arms_bounce_left {
  0% {
    transform: translate3d(0, var(--hover-top, 6px), 0) rotate(0deg); /* Reduced translation and kept rotation subtle */
  }
  100% {
    transform: translate3d(0, var(--hover-bottom, -6px), 0) rotate(-8deg);
  }
}

@keyframes arms_bounce_right {
  0% {
    transform: translate3d(0, var(--hover-top, 6px), 0) rotate(0deg); /* Reduced translation and kept rotation subtle */
  }
  100% {
    transform: translate3d(0, var(--hover-bottom, -6px), 0) rotate(8deg);
  }
}

@keyframes eyes_blink {
  0% {
    transform: scale(1, 1);
  }
  90% {
    transform: scale(1, 1);
  }
  95% {
    transform: scale(0.9, 0); /* Reduced scale change for smoother effect on smaller screens */
  }
  100% {
    transform: scale(1, 1);
  }
}


